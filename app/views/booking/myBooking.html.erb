 <body >
 <head>
    <%= stylesheet_link_tag    'booking', media: 'all', 'data-turbolinks-track': 'reload' %>

</head>
<div class="my Bookings">
 	<div class="row">
        <div class="col s12 m6">
          <div class="card white">
            <div class="card-content">
                  <span class="card-title">Booking Details</span>
                        
                        <table>
                              <tr>
                              <th>Checkin Date</th>
                              <th >Checkout Date</th>
                              <th >Booking Date</th>
                             </tr>
                              <%@bookingDetails.each do |book|%>
                             <tr>
                            <td><%= book.checkinDate %></td>
                            <td><%= book.checkoutDate %></td>
                            <td><%= book.bookingDate %></td>
                           </tr>
                           <%end%>

                        </table>
    
                    
              <%= paginate @bookingDetails %>
 	          </div>
        </div>
    </div>
</div>
</div>